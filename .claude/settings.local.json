{
  "permissions": {
    "allow": [
      "Bash(mkdir:*)",
      "Bash(npm install)",
      "Bash(npm run build:*)",
      "Bash(touch:*)",
      "Bash(chmod:*)",
      "Bash(npm run type-check:*)",
      "Bash(git init:*)",
      "Bash(git add:*)",
      "Bash(git config:*)",
      "Bash(gh auth:*)",
      "Bash(gh repo create:*)"
    ],
    "defaultMode": "acceptEdits"
  }
}